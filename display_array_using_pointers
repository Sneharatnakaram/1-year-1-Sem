#include<stdio.h>
void main()
{
int n,i,*ptr;
int a[20];
printf("enter number of elements in array");
scanf("%d",&n);
printf("enter the elements of array");
for(i=0;i<n;i++)
{
scanf("%d",&a[i]);
}
ptr=&a[0];
for(i=0;i<n;i++)
{
printf("%d\n",*(ptr+i));
}
return;
}


output


enter number of elements in array3
enter the elements of array1 2 3
1
2
3
